{
	"name" : "creifenscheid/rte-ckeditor-textpartlanguage",
	"type" : "typo3-cms-extension",
	"description" : "This TYPO3 extension implements the text part language feature for CKEditor 5",
	"license" : "GPL-2.0-or-later",
	"require" : {
		"typo3/cms-core" : ">=12.4.0 <12.4.8",
		"typo3/cms-rte-ckeditor" : ">=12.4.0 <12.4.8"
	},
	"require-dev": {
		"roave/security-advisories": "dev-latest"
	},
	"autoload" : {
		"psr-4" : {
			"CReifenscheid\\RteCkeditorTextpartlanguage\\" : "Classes/"
		}
	},
	"replace": {
		"typo3-ter/rte-ckeditor-textpartlanguage": "self.version"
	},
	"minimum-stability" : "stable",
	"authors" : [{
		"name" : "Christian Reifenscheid",
		"email" : "dev@christian-reifenscheid.de"
	}],
	"extra": {
		"typo3/cms": {
			"extension-key": "rte_ckeditor_textpartlanguage"
		}
	}
}
